{
  "name": "ajanuw-vite-build-esm",
  "version": "1.0.0",
  "type": "module",
  "description": "A modern TypeScript utility library with ESM support, built with Vite. Provides math utilities with tree-shaking friendly exports.",
  "main": "./dist/umd/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "unpkg": "./dist/umd/index.js",
  "jsdelivr": "./dist/umd/index.js",
  "homepage": "https://github.com/januwA/ajanuw-vite-build-esm#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/januwA/ajanuw-vite-build-esm.git"
  },
  "bugs": {
    "url": "https://github.com/januwA/ajanuw-vite-build-esm/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/umd/index.js",
      "default": "./dist/esm/index.js"
    },
    "./add": {
      "types": "./dist/esm/add.d.ts",
      "import": "./dist/esm/add.js",
      "default": "./dist/esm/add.js"
    },
    "./sub": {
      "types": "./dist/esm/sub.d.ts",
      "import": "./dist/esm/sub.js",
      "default": "./dist/esm/sub.js"
    },
    "./foo": {
      "types": "./dist/esm/foo.d.ts",
      "import": "./dist/esm/foo.js",
      "default": "./dist/esm/foo.js"
    },
    "./react-shared": {
      "types": "./dist/esm/react-shared.d.ts",
      "import": "./dist/esm/react-shared.js",
      "default": "./dist/esm/react-shared.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:umd",
    "build:esm": "vite build",
    "build:umd": "BUILD_UMD=true vite build",
    "build:prod": "npm run build:esm:prod && npm run build:umd:prod",
    "build:esm:prod": "vite build --mode production",
    "build:umd:prod": "BUILD_UMD=true vite build --mode production",
    "dev": "vite build --watch",
    "preview": "vite preview",
    "prepublishOnly": "npm run build:prod"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.6.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@vitejs/plugin-react": "^5.0.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.5.3",
    "zustand": "^5.0.8"
  },
  "keywords": [
    "typescript",
    "vite",
    "esm",
    "utils",
    "math",
    "library",
    "tree-shaking",
    "modular",
    "javascript"
  ],
  "author": {
    "name": "januwA",
    "url": "https://github.com/januwA"
  },
  "license": "MIT"
}
